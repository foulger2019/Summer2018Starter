Cell[BoxData[
 RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"FormFunction", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\"\<name\>\"", ",", "\"\<What is your name?\>\""}], "}"}], 
        "\[Rule]", "\"\<String\>\""}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\"\<age\>\"", ",", "\"\<How old are you?\>\""}], "}"}], 
        "\[Rule]", "\"\<Number\>\""}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\"\<gender\>\"", ",", " ", "\"\<What is your gender?\>\""}],
          "}"}], " ", "\[Rule]", " ", 
        RowBox[{"{", 
         RowBox[{
         "\"\<Male\>\"", ",", "\"\<Female\>\"", ",", "\"\<Other\>\""}], 
         "}"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\"\<height\>\"", ",", "\"\<How tall are you in cm?\>\""}], 
         " ", "}"}], "\[Rule]", " ", "\"\<Number\>\""}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
         "\"\<weight\>\"", ",", "\"\<How much do you weigh in lbs?\>\""}], 
         " ", "}"}], "\[Rule]", " ", "\"\<Number\>\""}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
         "\"\<exercise\>\"", ",", " ", 
          "\"\<How many minutes of exercise do you do a day?\>\""}], "}"}], 
        "\[Rule]", "\"\<Number\>\""}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
         "\"\<water\>\"", ",", 
          "\"\<How many glasses of water did you drink yesterday?\>\""}], 
         "}"}], " ", "\[Rule]", " ", "\"\<Number\>\""}], ",", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
         "\"\<pets\>\"", ",", " ", "\"\<How many pets do you own?\>\""}], 
         "}"}], "\[Rule]", "\"\<Number\>\""}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
         "\"\<health\>\"", ",", " ", "\"\<How healthy do you feel?\>\""}], 
         "}"}], " ", "\[Rule]", " ", 
        RowBox[{"{", 
         RowBox[{
         "\"\<Very Unhealthy\>\"", ",", "\"\<Unhealthy\>\"", ",", 
          "\"\<Healthy\>\"", ",", "\"\<Very Healthy\>\""}], "}"}]}]}], 
      "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"DatabinAdd", "[", 
       RowBox[{"bin", ",", 
        RowBox[{"<|", 
         RowBox[{
          RowBox[{"\"\<name\>\"", "\[Rule]", "#name"}], " ", ",", 
          RowBox[{"\"\<age\>\"", "\[Rule]", "#age"}], ",", " ", 
          RowBox[{"\"\<gender\>\"", " ", "\[Rule]", "#gender"}], ",", " ", 
          RowBox[{"\"\<height\>\"", " ", "\[Rule]", "#height"}], ",", " ", 
          RowBox[{"\"\<weight\>\"", " ", "\[Rule]", "#weight"}], ",", " ", 
          RowBox[{"\"\<exercise\>\"", " ", "\[Rule]", "#exercise"}], ",", " ", 
          RowBox[{"\"\<water\>\"", " ", "\[Rule]", "#water"}], ",", " ", 
          RowBox[{"\"\<pets\>\"", " ", "\[Rule]", "#pets"}], ",", " ", 
          RowBox[{"\"\<health\>\"", " ", "\[Rule]", " ", "#health"}]}], 
         "|>"}]}], "]"}], "&"}]}], "]"}], ",", "\[IndentingNewLine]", 
   RowBox[{"Permissions", " ", "\[Rule]", " ", "\"\<Public\>\""}]}], 
  "]"}]], "Input",
 CellChangeTimes->{3.739915578388522*^9},
 CellID->907882076,ExpressionUUID->"9252fba3-0145-49ae-93cf-fabd0b1d609c"],

